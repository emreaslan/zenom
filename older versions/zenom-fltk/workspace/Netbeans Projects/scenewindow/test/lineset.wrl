#VRML V2.0 utf8
DEF TRANSFORM Transform { children [
  Shape {
    geometry  DEF B IndexedLineSet {
      coord Coordinate { point [ 0 10 0, 0 0 0, 20 0 0, -1 5 0, 21 5 0 ] }
      coordIndex [ 0 1 2 -1 3 4 -1]
      color Color { color [ 1 0 0, 0 1 0 ] }
      colorIndex [ 0 1 ]
      colorPerVertex FALSE 
    }
  }
  DEF SHAPE Shape {
    appearance DEF C Appearance {
          material Material { emissiveColor 0.0 0.0 1.0 }
        }
    geometry DEF A IndexedLineSet {
      coord Coordinate { point [ 2 1 0, 5 2 0, 8 1.5 0, 11 9 0, 14 7 0, 17 10 0 ] }
      coordIndex [ 0 1 2 3 4 5 ]     # connect the dots
    }
  }
]} 



Transform {
    translation 0 0 -2.5
    children [ 
      USE TRANSFORM
    ]
}

Transform {
    translation 0 0 -1.5
    children [ 
      USE TRANSFORM
    ]
}
